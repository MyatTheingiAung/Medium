<!--Profile Modal -->
<div class="modal fade" id="profileModal" tabindex="-1" aria-labelledby="image.png" aria-hidden="true">
  <div class="modal-dialog modal-dialog-centered">
    <div class="modal-content">
        <div class="modal-body ps-5">
            <div class="d-flex justify-content-between align-items-center pt-3 mb-5">
                <h1 class="modal-title fs-4" id="image.png">Profile Information</h1>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <%= form_with do |form| %>
                <%= form.label :image, "Photo", class:"mb-3" %>
                <div class="mb-4">
                    <%= image_tag "image.jpg",height: 70, class:"rounded-circle me-3" %>
                    <a class="text-decoration-none text-success fs-6 me-2 d-inline-block" href="#">Update</a>
                    <a class="text-decoration-none text-danger" href="#">Remove</a>
                </div>
                <%= form.label :name, "Name", class:"mb-2" %>
                <%= form.text_field :name, class:"w-100 border-start-0 border-end-0 border-top-0 py-2 border-bottom-1 mb-4", placeholder:"Your Name", style:"outline:none;border-color:#ced4da" %>
                <%= form.label :password, "Password", class:"mb-2" %>
                <%= form.password_field :password, class:"w-100 border-start-0 border-end-0 border-top-0 py-2 border-bottom-1 mb-4", placeholder:"Your Password", style:"outline:none;border-color:#ced4da" %>
                <%= form.label :bio, "Bio", class:"mb-2" %>
                <%= form.text_area :bio, class:"w-100 border-start-0 border-end-0 border-top-0 py-2 border-bottom-1 mb-4", placeholder:"Your Bio", style:"outline:none;border-color:#ced4da" %>
                <div class="d-flex justify-content-end">
                    <a href="" class="d-inline-block" data-bs-dismiss="modal" aria-label="Close"><span class="rounded-pill bg-white border border-success text-success d-inline-block me-3 px-4 py-2">Cancle</span></a>
                    <%= form.submit 'Save' , class:"rounded-pill bg-success text-white d-inline-block border-0 px-4 py-2" %>
                </div>
            <% end %>
        </div>
    </div>
  </div>
</div>
<!--Pssword Modal -->
<div class="modal fade" id="passwordModal" tabindex="-1" aria-labelledby="image.png" aria-hidden="true">
  <div class="modal-dialog modal-dialog-centered">
    <div class="modal-content">
        <div class="modal-body ps-5">
            <div class="d-flex justify-content-between align-items-center pt-3 mb-5">
                <h1 class="modal-title fs-4" id="image.png">Change Password</h1>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <%= form_with do |form| %>
                <%= form.label :oldpass, "Old Password", class:"mb-2" %>
                <%= form.password_field :oldpass, class:"w-100 border-start-0 border-end-0 border-top-0 py-2 border-bottom-1 mb-4", placeholder:"Old Password", style:"outline:none;border-color:#ced4da" %>
                <%= form.label :newpass, "New Password", class:"mb-2" %>
                <%= form.password_field :newpass, class:"w-100 border-start-0 border-end-0 border-top-0 py-2 border-bottom-1 mb-4", placeholder:"New Password", style:"outline:none;border-color:#ced4da" %>
                <%= form.label :confpass, "Confirm Password", class:"mb-2" %>
                <%= form.password_field :confpass, class:"w-100 border-start-0 border-end-0 border-top-0 py-2 border-bottom-1 mb-4", placeholder:"Confirm Password", style:"outline:none;border-color:#ced4da" %>
                <div class="d-flex justify-content-end">
                    <a class="d-inline-block" data-bs-dismiss="modal" aria-label="Close"><span class="rounded-pill bg-white border border-success text-success d-inline-block me-3 px-4 py-2">Cancle</span></a>
                    <%= form.submit 'Save' , class:"rounded-pill bg-success text-white d-inline-block border-0 px-4 py-2" %>
                </div>
            <% end %>
        </div>
    </div>
  </div>
</div>
